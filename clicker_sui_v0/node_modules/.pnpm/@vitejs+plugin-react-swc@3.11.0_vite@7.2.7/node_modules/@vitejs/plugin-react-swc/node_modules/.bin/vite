#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/teodor/BitEater_SUI_HACK/clicker_sui_v0/node_modules/.pnpm/vite@7.2.7/node_modules/vite/bin/node_modules:/home/teodor/BitEater_SUI_HACK/clicker_sui_v0/node_modules/.pnpm/vite@7.2.7/node_modules/vite/node_modules:/home/teodor/BitEater_SUI_HACK/clicker_sui_v0/node_modules/.pnpm/vite@7.2.7/node_modules:/home/teodor/BitEater_SUI_HACK/clicker_sui_v0/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/teodor/BitEater_SUI_HACK/clicker_sui_v0/node_modules/.pnpm/vite@7.2.7/node_modules/vite/bin/node_modules:/home/teodor/BitEater_SUI_HACK/clicker_sui_v0/node_modules/.pnpm/vite@7.2.7/node_modules/vite/node_modules:/home/teodor/BitEater_SUI_HACK/clicker_sui_v0/node_modules/.pnpm/vite@7.2.7/node_modules:/home/teodor/BitEater_SUI_HACK/clicker_sui_v0/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vite@7.2.7/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../../vite@7.2.7/node_modules/vite/bin/vite.js" "$@"
fi
